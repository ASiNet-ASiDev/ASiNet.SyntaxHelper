<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>$(AssemblyName)</Title>
    <Version>0.0.1</Version>
    <Authors>ASiDev</Authors>
    <Description>Provides paths to system assemblys in a convenient format.</Description>
    <Copyright>MIT</Copyright>
    <RepositoryUrl>https://github.com/ASiNet-ASiDev/ASiNet.SyntaxHelper</RepositoryUrl>
    <PackageTags>assemblys, paths, tool, helper</PackageTags>
  </PropertyGroup>

</Project>
